<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>vhm</display-name>
    <servlet>
        <servlet-name>servletexamen</servlet-name>
        <servlet-class>com.mycompany.vhm.servletexamen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clasedeapoyo</servlet-name>
        <servlet-class>com.mycompany.vhm.clasedeapoyo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cliente</servlet-name>
        <servlet-class>com.mycompany.vhm.cliente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletexamen</servlet-name>
        <url-pattern>/resultados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clasedeapoyo</servlet-name>
        <url-pattern>/evaluacion.java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cliente</servlet-name>
        <url-pattern>/evaluacion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
